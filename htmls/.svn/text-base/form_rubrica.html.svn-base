<input type='text' id="responsabile" /> *</div>
</div><br><br>

<!-- email -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Email:</div><div style='float:left;'>
<input type='text' id="email" /> *</div>
</div><br><br />

<!-- telefono -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Telefono:</div><div style='float:left;'>
<input type='text' id="telefono" /> *</div>
</div><br><br>

<!-- fax-->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Fax:</div><div style='float:left;'>
<input type='text' id="fax" /> *</div>
</div><br><br>

<!-- cellulare -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Cellulare:</div><div style='float:left;'>
<input type='text' id="cellulare" /> *</div>
</div><br><br>

<!-- attività -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Attività cliente:</div><div style='float:left;'>
<textarea id="attivita" rows="4" cols="16"></textarea>*</div>
</div><br><br>

<!-- banca -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Banca:</div><div style='float:left;'>
<input type='text' id="banca" /></div>
</div><br><br>

<!-- iban -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Iban:</div><div style='float:left;'>
<input type='text' id="iban" /></div>
</div><br><br>

<!-- agente -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Agente:</div><div style='float:left;'>
<input type='text' id="agente" /></div>
</div><br><br>

<!-- regione -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Regione:</div><div style='float:left;'>
<input type='text' id="regione_agente" /></div>
</div><br><br>

<!-- provincia -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Provincia:</div><div style='float:left;'>
<input type='text' id="provincia_agente" /></div>
</div><br><br>

<!-- capo area -->
<div style='clear:both;'>
<div style='float:left;width:250px;text-align:right;padding-right:10px;height:30px;line-height:30px;height:30px;line-height:30px;'>Capo area:</div><div style='float:left;'>
<input type='text' id="capo_area" /></div>
</div><br><br>

<!-- Indirizzo del cantiere -->

<!-- denominazione -->
